{
  "name": "epicbot-desktop",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "start:renderer": "vite --config desktop/vite.config.js",
    "start:electron": "cross-env UI_DEV=1 wait-on http://localhost:5173 && electron desktop/electron/main.js",
    "dev": "concurrently -k -n RENDERER,ELECTRON \"npm:start:renderer\" \"npm:start:electron\"",
    "build:renderer": "vite build --config desktop/vite.config.js"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mantine/core": "7.13.3",
    "@mantine/hooks": "7.13.3",
    "@tabler/icons-react": "3.19.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.26.2"
  },
  "devDependencies": {
    "@types/node": "22.7.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.4",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "electron": "31.3.1",
    "typescript": "5.6.2",
    "vite": "5.4.10",
    "wait-on": "7.2.0"
  }
} 